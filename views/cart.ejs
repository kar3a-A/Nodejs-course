
<h2>Cart <br> Items</h2>
<% if (items.length) { %>
    <% items.forEach(item => { %>
        <div class="blog-card">
            <h3>Product: <%= item.product %> </h3>
            <h3>Price: <%= item.price %></h3>
        </div>
    <% }) %>
<% } else { %>
    <h4>Items are out of stock today</h4>
<% } %>
